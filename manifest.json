{
    "name": "배봉탕",
    "description": "시립대 즐겨찾기 모음",
    "version": "1.0.0",
    "manifest_version": 2,
    "icons": {
        "16": "./assets/baebongtang.png",
        "48": "./assets/baebongtang.png",
        "128": "./assets/baebongtang.png"
    },
    "browser_action": {
        "default_popup": "./popup.html",
        "default_icon": "./assets/baebongtang.png"
    },
    "permissions": [
        "tabs",
        "<all_urls>"
    ]
}